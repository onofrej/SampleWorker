{
  "AppName": "SampleWorker",
  "AWS": {
    "ServiceURL": "http://localhost:4566"
  },
  "kafka": {
    "SchemaRegistryUrl": "http://localhost:8081",
    "BootstrapServer": "localhost:9093",
    "Company": {
      "Initials": "FD9",
      "Community": "CSI"
    },
    "Consumer": {
      "BatchSize": 1000,
      "ClientId": "SampleWorkerClientId",
      "ClientUser": "SampleWorkerClientId",
      "GroupId": "sampleworker",
      "Topics": {
        "Order": "order-placed-event"
      }
    }
  },
  "SecretsCache": {
    "CacheItemTTL": 3700000,
    "MaxCacheSize": 1024,
    "VersionStage": "AWSCURRENT"
  },
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithExceptionDetails"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "SampleWorker",
      "Initials": "FD9",
      "Community": "CSI"
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}